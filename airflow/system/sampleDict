FoamFile
{
		version     2.0;
		format      ascii;
		class       dictionary;
		location    "system";
		object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
		extrusionProbe
		{
				type cloud;
				axis xyz;
				points ((0 0 -0.002));
		}
);

fields          (p U);

